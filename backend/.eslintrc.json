{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "jest/globals": true
    },
    "extends": [
        "airbnb-base",
        "plugin:jest/recommended"
    ],
    "plugins": [
        "jest"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "comma-dangle": 0,
        "no-console": 0,
        "object-shorthand": 0
    }
}
